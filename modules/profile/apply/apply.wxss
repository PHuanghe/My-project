/* pages/Myhome/apply/apply.wxss */

.apply_data {
  padding: 50rpx 34rpx;
  font-size: 26rpx;
  color:#333;

}

.data_image {
  width: 100%;
  height: 400rpx;
  margin: 50rpx 0;
}

.data_image image {
  width: 100%;
  height: 100%;
}

.data_content {
  line-height: 60rpx;
}

.apply_tabbar {
  width: 100%;
  display: flex;
  position: fixed;
  bottom: 0;
  left: 0;
  font-size: 26rpx;
  text-align: center;
  height: 88rpx;
  line-height: 88rpx;
}

.tabbar_phone {
  flex: 1;
  color:#f27c27;
  background-color: white;
}

.tabbar_please {
  flex: 1;
  color: #fff;
  background-color:#f74a38;
}
